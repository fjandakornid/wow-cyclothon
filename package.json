{
  "name": "wow-cyclothon",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "^4.3.1",
    "node-sass-chokidar": "^1.3.4",
    "npm-run-all": "^4.1.5",
    "google-maps-api-loader": "^1.1.1",
    "kd-tree-js": "0.0.1",
    "react": "^16.8.3",
    "react-dom": "^16.8.3",
    "react-error-boundary": "^1.2.3",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.1.5",
    "rimraf": "^2.6.3"
  },
  "scripts": {
    "start-js": "rimraf ./build && react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "test-cov": "react-scripts test --env=jsdom --coverage",
    "build-css": "node-sass-chokidar ./src/styles -o ./src/styles",
    "watch-css": "npm run build-css && node-sass-chokidar ./src/styles -o ./src/styles --watch --recursive"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
